{
  "expo": {
    "name": "ATSUME - スポーツイベント管理",
    "slug": "atsume",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "atsume",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.atsume.app",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "プロフィール画像のアップロードに使用します",
        "NSPhotoLibraryUsageDescription": "イベント画像のアップロードに使用します"
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.atsume.app",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "atsume"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.ico",
      "bundler": "metro",
      "name": "ATSUME",
      "shortName": "ATSUME",
      "themeColor": "#3B82F6",
      "backgroundColor": "#ffffff"
    },
    "extra": {
      "eas": {
        "projectId": "2e72e6f1-c886-4bb1-8e46-91add7dc6114"
      }
    },
    "owner": "aireal"
  }
}
